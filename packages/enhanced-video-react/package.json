{
    "name": "@plurid/enhanced-video-react",
    "version": "0.1.6",
    "description": "Enhanced Video HTML Element, React implementation",
    "keywords": [
        "enhanced",
        "video",
        "html",
        "react"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/enhanced-video"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/enhanced-video/issues"
    },
    "homepage": "https://github.com/plurid/enhanced-video/blob/master/packages/enhanced-video-react",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "jsnext:main": "distribution/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "storybook": "start-storybook -p 9001 -s ./test -c storybook",
        "build.storybook": "build-storybook -c storybook",
        "clean": "rm -rf ./distribution",
        "spec": "jest",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.development": "yarn clean && rollup -c --environment MODE_ENV:local",
        "build.production": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn clean && yarn lint && yarn spec && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-themes": "*",
        "apollo-cache-inmemory": ">=1.6",
        "apollo-client": ">=2.6",
        "apollo-link": ">=1.2",
        "apollo-link-error": ">=1.1",
        "apollo-link-http": ">=1.5",
        "graphql": ">=14",
        "graphql-tag": ">=2.10",
        "react": ">=16",
        "react-apollo": ">=3.1",
        "react-dom": ">=16",
        "styled-components": ">=4"
    },
    "devDependencies": {
        "@babel/core": "^7.10.3",
        "@plurid/plurid-themes": "^0.1.26",
        "@rollup/plugin-babel": "^5.0.3",
        "@rollup/plugin-url": "^5.0.1",
        "@storybook/addon-actions": "^5.3.19",
        "@storybook/addon-info": "^5.3.19",
        "@storybook/addon-knobs": "^5.3.19",
        "@storybook/addon-links": "^5.3.19",
        "@storybook/addons": "^5.3.19",
        "@storybook/react": "^5.3.19",
        "@svgr/rollup": "^5.4.0",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.0",
        "@types/react": "^16.9.38",
        "@types/react-dom": "^16.9.8",
        "@types/styled-components": "^5.1.0",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "apollo-cache": "^1.3.5",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link": "^1.2.14",
        "apollo-link-error": "^1.1.13",
        "apollo-link-http": "^1.5.17",
        "apollo-utilities": "^1.3.4",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^7.3.0",
        "graphql": "^15.1.0",
        "graphql-tag": "^2.10.3",
        "jest": "^26.0.1",
        "react": "^16.13.1",
        "react-apollo": "^3.1.5",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.13.1",
        "regenerator-runtime": "^0.13.5",
        "rollup": "^2.17.1",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-postcss": "^3.1.2",
        "rollup-plugin-typescript2": "^0.27.1",
        "styled-components": "^5.1.1",
        "ts-jest": "^26.1.0",
        "typescript": "^3.9.5"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__specs__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "collectCoverage": false,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/distribution/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "source/*.{js,ts}"
        ]
    }
}
