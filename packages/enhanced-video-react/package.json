    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/enhanced-video/issues"
    },
    "homepage": "https://github.com/plurid/enhanced-video/blob/master/packages/enhanced-video-react",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.js",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "storybook": "start-storybook -p 9001 -s ./test -c storybook",
        "build.storybook": "build-storybook -c storybook",
        "clean": "rm -rf ./distribution",
        "clean.tests": "find ./distribution -type d -name '__tests__' -exec rm -r {} +",
        "clean.stories": "find ./distribution -type d -name '__stories__' -exec rm -r {} +",
        "test": "jest -c ./configurations/jest.config.js",
        "lint": "eslint -c ./configurations/.eslintrc.js ./source",
        "start": "yarn clean && rollup -c ./scripts/rollup.config.js -w --environment ENV_MODE:local",
        "build.local": "yarn clean && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:local",
        "build.development": "yarn clean && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:development",
        "build.production": "yarn clean && yarn lint && yarn test && rollup -c ./scripts/rollup.config.js --environment ENV_MODE:production && yarn clean.tests && yarn clean.stories",
        "build": "yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@apollo/client": ">=3.1",
        "@plurid/plurid-functions": "*",
