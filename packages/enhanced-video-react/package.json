{
    "name": "@plurid/enhanced-video-react",
    "version": "0.1.5",
    "description": "Enhanced Video HTML Element, React implementation",
    "keywords": [
        "enhanced",
        "video",
        "html",
        "react"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/enhanced-video"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/enhanced-video/issues"
    },
    "homepage": "https://github.com/plurid/enhanced-video/blob/master/packages/enhanced-video-react",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "jsnext:main": "distribution/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "spec": "jest",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.development": "yarn clean && rollup -c --environment MODE_ENV:local",
        "build.production": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn clean && yarn lint && yarn spec && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/plurid-themes": "*",
        "apollo-cache-inmemory": ">=1.6",
        "apollo-client": ">=2.6",
        "apollo-link": ">=1.2",
        "apollo-link-error": ">=1.1",
        "apollo-link-http": ">=1.5",
        "graphql": ">=14",
        "graphql-tag": ">=2.10",
        "react": ">=16",
        "react-apollo": ">=3.1",
        "react-dom": ">=16",
        "styled-components": ">=4"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@plurid/plurid-themes": "^0.1.0",
        "@svgr/rollup": "^4.3.3",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.23",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.4",
        "@types/styled-components": "^4.4.0",
        "@typescript-eslint/eslint-plugin": "^2.8.0",
        "@typescript-eslint/parser": "^2.8.0",
        "apollo-cache": "^1.3.2",
        "apollo-cache-inmemory": "^1.6.0",
        "apollo-client": "^2.6.0",
        "apollo-link": "^1.2.11",
        "apollo-link-error": "^1.1.10",
        "apollo-link-http": "^1.5.14",
        "apollo-utilities": "^1.3.2",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^6.6.0",
        "graphql": "^14.5.6",
        "graphql-tag": "^2.10.1",
        "jest": "^24.9.0",
        "react": "^16.8.3",
        "react-apollo": "^3.1.1",
        "react-dom": "^16.8.3",
        "react-test-renderer": "^16.12.0",
        "rollup": "^1.27.2",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.25.2",
        "rollup-plugin-url": "^3.0.0",
        "styled-components": "^4.1.3",
        "ts-jest": "^24.1.0",
        "typescript": "^3.7.2"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__specs__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "collectCoverage": false,
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/distribution/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "source/*.{js,ts}"
        ]
    }
}
